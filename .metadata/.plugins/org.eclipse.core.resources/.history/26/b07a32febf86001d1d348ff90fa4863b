class lect
{
	int n1,n2,res;
	
	public int perform(int a, int b)
	{
		res = (n1+n2) * a + b;
		if(res>50)
			return 1;
		else
			return 0;
	}
}

public class Constructor_Lect {

	public static void main(String[] args) 
	{
		lect l = new lect();
		l.n1=4;
		l.n2=6;
		int k = l.perform(9, 4);
		System.out.println(k);
	}

}
