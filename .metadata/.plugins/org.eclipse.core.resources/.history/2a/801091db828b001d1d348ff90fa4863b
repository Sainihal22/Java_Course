class one
{
	int i;
	public void show() 
	{
		System.out.println(i);
		System.out.println("In One");

	}
}

class two extends one
{
//	@Override
	int i;
	public void show() 
	{
		super.i=8;
		super.show();
		System.out.println(i);
		System.out.println("In Two");

	}
}

public class OverridingLect 
{

	public static void main(String[] args) 
	{
//		two obj1 = new two();
		one obj1 = new two();
		obj1.show();

	}

}
