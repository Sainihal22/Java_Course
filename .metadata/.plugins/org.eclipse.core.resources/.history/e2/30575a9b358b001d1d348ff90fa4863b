class Calculator
{
	public int add(int i, int j)
	{
		return i+j;
	}
}

class CalcAdv
{
	public int add(int i, int j)
	{
		return i+j;
	}
	
	public int sub(int i, int j)
	{
		return i-j;
	}
}


public class InheritanceLect 
{

	public static void main(String[] args) 
	{
		CalcAdv obj = new CalcAdv();
		System.out.println(obj.add(3, 4));
		System.out.println(obj.sub(6, 3));

	}

}
